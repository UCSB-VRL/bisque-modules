<?xml version="1.0" encoding="utf-8"?>
<module name="EBSDSR" type="runtime">
             
     <tag name="inputs">
              
          <tag name="Input Image" type="resource">
               <template>
                    <tag name="label" value="Input Image"/>
                    <tag name="accepted_type" value="file"/>
                    <tag name="prohibit_upload" value="True"/>
               </template>
          </tag>
          <tag name="mex_url" type="system-input_resource"/>
          <tag name="bisque_token" type="system-input_resource"/>
     </tag>
         
     <tag name="outputs">
                  
          <tag name="NonImage">
                           
               <template>
                                    
                    <tag name="label" value="Outputs"/>
                                
                    <tag name="Output Arr" type="file"/>
               </template>
                       
          </tag>
              
          <tag name="Input Output IPF" type="image">
               <template>
                    <tag name="label" value="Input Output IPF"/>
               </template>
          </tag>
     </tag>
         
     <tag name="execute_options">
                  
          <tag name="iterable" value="image_url" type="dataset"/>
              
     </tag>
         
     <tag name="module_options">
                  
          <tag name="version" value="1"/>
              
     </tag>
         
     <tag name="display_options">
                 
          <tag name="group" value="Materials Science"/>
              
     </tag>
         
     <tag name="interface">
                  
          <tag name="javascript" type="file" value="webapp.js"/>
                  
          <tag name="css" type="file" value="webapp.css"/>
              
     </tag>
         
     <tag name="help" type="file" value="public/help.html"/>
         
     <tag name="thumbnail" type="file" value="public/thumbnail.png"/>
            
     <tag name="title" type="string" value="EBSDSR"/>
         
     <tag name="authors" type="string" value="Devendra"/>
         
     <tag name="description" type="string" value="EBSD Super-resolution"/>
</module>
